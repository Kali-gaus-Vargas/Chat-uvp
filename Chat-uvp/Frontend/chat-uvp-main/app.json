{
  "expo": {
    "name": "chatbot",
    "slug": "chatbot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.expo.chatbot",
      "config":{
        "googleSignIn": {
          "reservedClientId": "com.googleusercontent.apps.188689865659-otl662pp0bed3l5evujts55kqr9ik1sf"
        }
      },
      "googleServicesFile": "./google-services.json"
    },
    "android": {
      "package": "com.expo.chatbot",
      "googleServicesFile": "./google-services.json",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
